{
    "name": "chan/php-mvc-api",
    "description": "A PHP MVC framework for building RESTful APIs with MySQL support.",
    "type": "project",
    "require": {
        "php": "^8.3",
        "vlucas/phpdotenv": "^5.6",
        "firebase/php-jwt": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "enlightn/security-checker": "^2.0"
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
